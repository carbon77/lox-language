var globalX;
var globalY;

fun vector(x, y) {
    fun getX() { print x; }
    globalX = getX;

    fun getY() { print y; }
    globalY = getY;

    fun set(key, value) {
        if (key == "x") {
            x = value;
        } else {
            y = value;
        }
    }
    return set;
}

var set = vector(4, 1);

globalX();
globalY();

set("x", 4);
set("y", 90);

globalX();
globalY();
